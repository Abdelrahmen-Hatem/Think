{
  "name": "think-creative-agency",
  "version": "1.0.0",
  "description": "Landing page for Think Creative Agency",
  "main": "index.js",
  "scripts": {
    "build:sass": "sass src/scss/main.scss dist/css/main.css",
    "build:sass-min": "sass src/scss/main.scss dist/css/main.min.css --style compressed",
    "copy:js": "xcopy src\\js\\* dist\\js\\ /Y",
    "watch:sass": "sass src/scss/main.scss dist/css/main.css --watch",
    "dev": "npm-run-all --parallel watch:sass",
    "build": "npm run build:sass && npm run build:sass-min && npm run copy:js"

  },
  "keywords": [
    "agency",
    "landing-page",
    "creative",
    "sass",
    "responsive"
  ],
  "author": "Think Creative Agency",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.69.5",
    "npm-run-all": "^4.1.5"
  }
}